n      font-size: 16px;\n      text-decoration: none;\n      transition: 0.3s;\n      margin-bottom: 50px;\n    }\n    .shop-now-btn:hover {\n      background-color: #6a8800;\n      border-color: #6a8800;\n    }\n\n    /* ğŸ Coupon Cards */\n    .coupon-cards {\n      display: flex;\n      flex-wrap: wrap;\n      justify-content: center;\n      max-width: 1400px;\n      gap: 30px;\n      margin: 0 auto;\n    }\n\n    .coupon-card {\n      background-color: rgba(26, 26, 26, 0.55);\n      backdrop-filter: blur(12px);\n      border: 1px solid rgba(255, 255, 255, 0.15);\n      padding: 40px 30px;\n      border-radius: 20px;\n      width: calc(25% - 23px);\n      min-width: 280px;\n      min-height: 300px;\n      position: relative;\n      display: flex;\n      flex-direction: column;\n      justify-content: space-between;\n\n      /* Christmas glow border */\n      overflow: hidden;\n    }\n    .coupon-card::before {\n      content: '';\n      position: absolute;\n      inset: 0;\n      border-radius: 20px;\n      padding: 2px;\n      background: linear-gradient(\n        135deg,\n        rgba(255, 80, 80, 0.55),\n        rgba(0, 255, 120, 0.55)\n      );\n      -webkit-mask:\n        linear-gradient(#fff 0 0) content-box,\n        linear-gradient(#fff 0 0);\n      -webkit-mask-composite: xor;\n      mask-composite: exclude;\n      pointer-events: none;\n    }\n    .coupon-card::after {\n      content: '';\n      position: absolute;\n      top: -40px; left: 50%;\n      width: 140%;\n      height: 120px;\n      background: radial-gradient(circle, rgba(255,220,140,0.25), transparent);\n      transform: translateX(-50%);\n      pointer-events: none;\n    }\n\n    .coupon-card h3 {\n      font-size: 58px;\n      font-weight: 900;\n      background: linear-gradient(180deg, #f7ecd6, #c89b3c);\n      -webkit-background-clip: text;\n      -webkit-text-fill-color: transparent;\n      margin: 0;\n    }\n\n    .coupon-card p {\n      font-size: 18px;\n      color: #cccccc;\n    }\