te-columns: repeat(4, 1fr);\n    gap: 20px;\n    max-width: 1200px;\n    margin: 0 auto;\n  }\n\n  /* å¡ç‰‡æ ·å¼ */\n  .bf-product-card {\n    background: #1e1e24;\n    border: 1px solid #333;\n    border-radius: 8px;\n    overflow: hidden;\n    position: relative;\n    transition: transform 0.3s ease, box-shadow 0.3s ease;\n    display: flex;\n    flex-direction: column;\n  }\n\n  .bf-product-card:hover {\n    transform: translateY(-5px);\n    box-shadow: 0 10px 20px rgba(0,0,0,0.5);\n    border-color: #ffc857;\n  }\n\n  /* æ ‡ç­¾ */\n  .badge-overlay {\n    position: absolute;\n    top: 10px;\n    left: 10px;\n    background: #ffc857;\n    color: #000;\n    padding: 4px 8px;\n    font-size: 0.8em;\n    font-weight: bold;\n    border-radius: 4px;\n    z-index: 2;\n  }\n\n  .prod-img {\n    width: 100%;\n    height: auto;\n    display: block;\n    aspect-ratio: 1 / 1; /* ä¿æŒæ­£æ–¹å½¢ */\n    object-fit: cover;\n    background: #fff; /* é˜²æ­¢é€æ˜å›¾ç‰‡çœ‹ä¸æ¸… */\n  }\n\n  .card-details {\n    padding: 15px;\n    display: flex;\n    flex-direction: column;\n    flex-grow: 1; /* è®©åº•éƒ¨å¯¹é½ */\n  }\n\n  .card-details h3 {\n    font-size: 1.1em;\n    margin: 0 0 10px 0;\n    color: #fff;\n    min-height: 40px; /* ä¿è¯ä¸¤è¡Œæ ‡é¢˜å¯¹é½ */\n  }\n\n  .price-row {\n    margin-bottom: 15px;\n  }\n\n  .sale-price {\n    color: #ffc857;\n    font-size: 1.4em;\n    font-weight: bold;\n    margin-right: 10px;\n  }\n\n  .old-price {\n    color: #888;\n    text-decoration: line-through;\n    font-size: 0.9em;\n  }\n\n  /* åº“å­˜æ¡æ ·å¼æ ¸å¿ƒ */\n  .stock-container {\n    margin-top: auto; /* æ¨åˆ°åº•éƒ¨ */\n    margin-bottom: 15px;\n    background: rgba(255,255,255,0.05);\n    padding: 10px;\n    border-radius: 6px;\n    border: 1px solid #444;\n  }\n\n  .stock-text {\n    font-size: 0.85em;\n    color: #ddd;\n    margin-bottom: 6px;\n    display: flex;\n    align-items: center;\n  }\n\n  .stock-text strong {\n    color: #fff;\n  }\n\n  .blink-dot {\n    width: 8px;\n    height: 8px;\n    background-color: #ff4444;\n    border-radius: 50%;\n    display: inline-block;\n    margin-right: 6px;\n    animation: blink 1.5s infinite;\n  }\n\n  @keyframes blink {\n    0% { opacity: 1; }\n    50% { opacity: 0.4; }\n    100% { opacity: 1; }\n  }\n\n  .progress-bar-bg {\n    width: 100%;\n    height: 6px;\n    background-color: #333;\n    border-radius: 3px;\n    overflow: hidden;\n  }\n\n  .progress-bar-fill {\n    height: 100%;\n    background: linear-gradient(90deg, #ff4444, #ff8800);\n    width: 100%; /* JSæ§åˆ¶ */\n    transition: width 1s ease;\n  }\n\n  .buy-btn {\n    display: block;\n    width: 100%;\n    text-align: center;\n    background: #fff;\n    color: #000;\n    padding: 10px;\n    border-radius: 4px;\n    text-decoration: none;\n    font-weight: bold;\n    transition: background 0.3s;\n  }\n\n  .buy-btn:hover {\n    background: #ffc857;\n  }\n\n  /* å“åº”å¼ */\n  @media (max-width: 990px) {\n    .bf-card-grid {\n      grid-template-columns: repeat(2, 1fr); /* å¹³æ¿2åˆ— */\n    }\n  }\n\n  @media (max-width: 600px) {\n    .bf-card-grid {\n      grid-template-columns: 1fr; /* æ‰‹æœº1åˆ— */\n    }\n    .bf-encore-header h2 {\n      font-size: 1.8em;\n    }\n  }\n</style>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function() {\n  \n  // è®¾ç½®æ´»åŠ¨å¼€å§‹çš„åŸºå‡†æ—¶é—´ (å¹´, æœˆ-1, æ—¥, æ—¶)\n  // æŠ€å·§ï¼šè®¾ä¸ºå¤§æ¦‚2-3å¤©å‰ï¼Œè¿™æ ·ç°åœ¨çš„åº“å­˜çœ‹èµ·æ¥å·²ç»æ¶ˆè€—äº†ä¸€éƒ¨åˆ†\n  // æ¯”å¦‚è®¾ä¸º 2024å¹´11æœˆ28æ—¥\n  const startDate = new Date(2025, 11, 3, 8, 0, 0); // æ³¨æ„ï¼šæœˆä»½ä»0å¼€å§‹ï¼Œ10ä»£è¡¨11æœˆ\n  const now = new Date();\n\n  // è®¡ç®—è¿‡å»äº†å‡ å°æ—¶\n  const diffMs = now - startDate;\n  const diffHours = diffMs / (1000 * 60 * 60);\n\n  const stockContainers = document.querySelectorAll('.stock-container');\n\n  stockContainers.forEach(container => {\n    // è·å–åˆå§‹é…ç½®\n    const baseStock = parseInt(container.getAttribute('data-base-stock'));\n    const decayHours = parseInt(container.getAttribute('data-decay-hours'));\n\n    // è®¡ç®—åº”è¯¥å‡å°‘çš„æ•°é‡ï¼š (è¿‡å»çš„å°æ—¶æ•° / æ¯å¤šå°‘å°æ—¶å‡1)\n    // Math.floor ç¡®ä¿æ˜¯æ•´æ•°\n    let reduction = Math.floor(diffHours / decayHours * 1.5); // ä¹˜1.5ä¸ºäº†è®©å®ƒåœ¨æµ‹è¯•æ—¶çœ‹èµ·æ¥æ‰å¾—å¿«ä¸€ç‚¹ï¼Œä½ å¯ä»¥å»æ‰\n\n    // è®¡ç®—å½“å‰åº“å­˜\n    let currentStock = baseStock - reduction;\n\n    // å…œåº•é€»è¾‘ï¼šåº“å­˜ä¸èƒ½å°äº 3ï¼Œä¸ç„¶å¤ªå‡äº†æˆ–è€…å˜æˆ0äº†\n    if (currentStock < 3) {\n      currentStock = 3 + Math.floor(Math.random() * 3); // éšæœºæ˜¾ç¤º 3-5 ä¸ª\n    }\n\n    // è®¡ç®—è¿›åº¦æ¡ç™¾åˆ†æ¯”\n    // å‡è®¾åˆå§‹æ»¡é¢æ˜¯ baseStock + 20 (è¿™æ ·æ˜¾å¾—è¿›åº¦æ¡ä¸æ˜¯ä»100%å¼€å§‹ï¼Œæ›´çœŸå®)\n    const maxStock = baseStock + 10;\n    const percent = Math.max(5, Math.min(100, (currentStock / maxStock) * 100));\n\n    // æ›´æ–° DOM\n    const countEl = container.querySelector('.stock-count');\n    const barEl = container.querySelector('.progress-bar-fill');\n\n    if (countEl) countEl.textContent = currentStock;\n    if (barEl) {\n      barEl.style.width = percent + '%';\n      \n      // å¦‚æœåº“å­˜æä½ï¼Œè¿›åº¦æ¡å˜çº¢\n      if (percent < 20) {\n        barEl.style.background = '#ff0000';\n      }\n    }\n  });\n});\n</script>",
