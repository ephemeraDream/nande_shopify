irection: column;\n        justify-content: flex-start;\n    }\n\n    /* 视频保持比例 */\n    .video-container {\n        position: relative;\n        width: 100%;\n        padding-bottom: 56.25%;\n        height: 0;\n        background: #000;\n        margin-bottom: auto;\n    }\n    .embedded-video {\n        position: absolute;\n        top: 0;\n        left: 0;\n        width: 100%;\n        height: 100%;\n    }\n\n    /* 底部条完全贴底 */\n    .product-info-bar {\n        margin-top: 20px;\n        background: rgba(255,255,255,0.9);\n        padding: 15px;\n        border-radius: 5px;\n        color: #2c2c54;\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n    }\n\n    .price-badge {\n        background-color: #b33a00;\n        color: #fff;\n        padding: 3px 8px;\n        border-radius: 4px;\n        font-size: 0.7em;\n        margin-left: 10px;\n    }\n\n    .learn-more-button {\n        background-color: #ffc857;\n        color: #2c2c54;\n        padding: 10px 20px;\n        border-radius: 5px;\n        text-decoration: none;\n        font-weight: bold;\n    }\n\n    /* 时间轴价格盒子 */\n    .timeline-item {\n        margin-bottom: 15px;\n    }\n\n    .price-box {\n        display: flex;\n        align-items: center;\n        gap: 10px;\n        background: #ffc857;\n        padding: 30px 15px;\n        border-radius: 8px;\n    }\n\n    .price-box.inactive {\n        background: #4a4a6a;\n        color: #ccc;\n    }\n\n    /* 邮件表单 */\n    .email-signup-form-wrapper {\n        margin-top: 30px;\n        background: rgba(0,0,0,0.2);\n        padding: 15px;\n        border-radius: 5px;\n    }\n\n    .input-group {\n        display: flex;\n        width: 100%; /* 确保容器宽度填满 */\n    }\n\n    /* 移除了 .email-input 相关样式，因为元素已删除 */\n\n    .signup-button {\n        background: #000;\n        color: #fff;\n        border: none;\n        padding: 10px 20px;\n        font-weight: bold;\n        cursor: pointer;\n        /* 确保文字居中 */\n        text-align: center;\n        width: 100%;\n    }\n\n    /* 响应式 */\n    @media (max-width: 900px) {\n        .bf-promo-main-container {\n            flex-direction: column;\n        }\n        .bf-promo-left-panel {\n            padding-right: 0;\n        }\n    }\n\n    /* 让左右两栏永远等高 —— 关键修复 */\n    .bf-promo-main-container {\n        align-items: stretch !important;\n    }\n\n    /* 让右侧整栏自动拉伸到底部，与左侧齐平 */\n    .bf-promo-right-panel {\n        display: flex;\n        flex-direction: column;\n    }\n\n    /* 让右侧按钮栏自动贴到底部（关键） */\n    .product-info-bar {\n        margin-top: auto !important;\n    }\n</style>\n\n<script>\ndocument.addEventListener(\"DOMContentLoaded\", function () {\n    const btn = document.querySelector(\".open-omnisend-popup\");\n    if (btn) {\n        btn.addEventListener(\"click\", function (e) {\n            e.preventDefault(); // 阻止默认行为\n            \n            // 使用你指定的更稳健的代码\n            window.omnisend = window.omnisend || [];\n            window.omnisend.push(['openForm', '691db9655000977f5041e114']);\n        });\n    }\n});\n</script>",
