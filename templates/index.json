        "swiper_GfiKcC": {
          "type": "swiper",
          "settings": {
            "name": "@CH3 - Charly Charles Check",
            "introduce": "",
            "description": "<p>\"Ich fand es besonders gut und es sieht wirklich cool aus. Es ist einfach ideal fur lange Arbeitstage oder Gaming-Sessions, bei         denen Komfort und Ergonomie wichtig sind.\"</p>",
