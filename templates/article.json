{
    "sections":{
        "breadcrumbs": {
            "type": "breadcrumbs",
            "settings": {
                "custom_breadcrumb": "",
            },
            "custom_css": [
                ".breadcrumb-con{top: 0px;position:relative}",
                ".breadcrumb,.breadcrumb a,.breadcrumb span {color: #7f7e75;}"
            ],
        },
        "main":{
            "type":"main-article",
            "blocks":{
                "featured_image":{
                    "type":"featured_image",
                    "settings":{"image_height":"adapt"}
                },
                "title":{
                    "type":"title",
                    "settings":{
                        "blog_show_date":true,"blog_show_author":false
                    }
                },
                "share":{
                    "type":"share",
                    "settings":{
                        "share_label":"Share"
                    }
                },
                "content":{
                    "type":"content","settings":{}
                }
            },
            "block_order":[
                "featured_image",
                "title",
                "share",
                "content"
            ],
            "settings":{}
        }
    },
    "order":[
        "breadcrumbs",
        "main"
    ]
}